extends ../fundContainer
block fundTitle
  title 基金赞助
block fundContent
  .container-fluid.max-width
    #data.hidden=objToStr({fundId: data.fundId, money: data.money, refund: data.refund})
    .cox-xs-12.col-md-6.col-md-offset-3.box-shadow-panel
      #donationApp(v-cloak)
        donation(:fund="fundId" :inimoney="money" :refund="refund")
block fundScripts
  +includeJS('/fund/donation/donation.js')
