if !isIncluded('formNav')
  style.
    .fund-form-nav>*{
      font-size: 1.4rem;
      font-weight: 700;
      color: #333;
      margin: 0 0.3rem;
    }
    .fund-form-nav>*:hover, .fund-form-nav>*:active{
      color: #333;
    }
  mixin formNav(form, fundName)
    -var {fund, code, _id} = form;
    .fund-form-nav.text-center
      a(href='/fund')=fundName || '基金'
      | /
      a(href=`/fund/list/${fund._id}`)=fund.name
      | /
      a(href=`/fund/a/${_id}`)=code
