                .mdui-grid-tile-text
                  .mdui-grid-tile-title(style="font-size:13px") {{all.oname}}
                .mdui-grid-tile-buttons(v-bind:style="['png', 'jpg'].indexOf(all.ext) > -1 ? 'display:block' : 'display:none'")
                  button.mdui-btn.mdui-btn-icon(@click="pictureEdit(all.rid)")
                    i.fa.fa-pencil-square-o
