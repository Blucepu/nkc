style.
  [v-vloak]{display:none}
style.
  .pastearea {
    border-color: #9baec8;
    border-width: 2px;
    border-style: dashed;
    background-color: #d9e1e8;
    //height: 60px;
    //margin-top:10px;
    //margin-bottom:-15px;
  }
  .uploadFileFail {
    color: red;
  }
  .mdui-table td,.mdui-table th {
    padding: 0;
  }
  .mdui-table td:first-child, .mdui-table th:first-child {
    padding: 0;
  }
  .mdui-btn {
    height: 32px;
    min-width: 1px;
  }
  .mdui-blue-color {
    color: #fff !important;
    background-color: #2b90d9 !important;
  }
