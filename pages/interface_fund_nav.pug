-const {fundList} = data;
.fund-nav
  ol.breadcrumb
    li
      a(href="/fund") 首页
    span &nbsp;&nbsp;&nbsp;
    //li
    //  a(href="/fund/info") 科创基金
    //span &nbsp;&nbsp;&nbsp;
    span
      a(href=`/fund/me`) 我的基金&nbsp;
      if data.fundNotify > 0
        span.redFontColor= data.fundNotify
    span &nbsp;&nbsp;
    li
      a(href=`/fund/list`) 基金列表
    if data.fund
      li
        a(href=`/fund/list/${data.fund._id.toLowerCase()}`)= data.fund.name
    if data.applicationForm && data.applicationForm.code
      li
        a(href=`/fund/a/${data.applicationForm.code}`)= data.applicationForm.code
    if data.bill
      li
        a(href=`/fund/list/${data.fund._id.toLowerCase()}/bills`)= `基金账单`
    if data.nav
      li= data.nav
    if data.fundSettings.readOnly
      h3(style='color: #888;') 科创基金已开启只读模式
