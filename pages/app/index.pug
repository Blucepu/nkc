doctype html
html(lang='zh-CN')
  head
    meta(charset='UTF-8')
    meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(href="/bootstrap/css/bootstrap.css" rel='stylesheet' type='text/css')
    link(rel='stylesheet' href='/font-awesome/css/font-awesome.css')
    link(rel='shortcut icon' href='/favicon.ico')
    link(href="/app/index.css" rel='stylesheet' type='text/css')
    title 科创
  body
    mixin logo
      div.logo-bottom
        img(src='/favicon.ico')
        span= data.serverSettings.websiteName
    .container#app(v-cloak)
      .row(v-if='toBrowser')
        .col-xs-12.col-md-12
          div.info 请点击右上角选择在浏览器打开
          +logo
      .row(v-else)
        .col-xs-12.col-md-12.text-center
          //.logo(@click="toHome")= data.serverSettings.websiteName
          a.link(href='/appDownload/android/latest') 点击下载
          +logo

    script(src='/jquery/jquery-1.11.1.js')
    script(src='/bootstrap/js/bootstrap.min.js')
    script(src='/Promise/promise.min.js')
    script(src='/vue/dist/vue.js')
    script(src=`/interface_common.js?v=${global.NKC.startTime}`)
    script(src=`/app/index.js?v=${global.NKC.startTime}`)
