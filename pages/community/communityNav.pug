if !isIncluded('communityNav')
  mixin communityNav(arr)
    a.m-r-05(href='/c' style='font-size: 1.25rem;') 论坛
    for item, index in arr
      span.m-r-05 /
      if item.url
        a.m-r-05(href=url title=item.title style='font-size: 1.25rem;')=item.name
      else
        span.m-r-05(title=item.title style='font-size: 1.25rem;')=item.name
