+includeCSS("/publicModules/apps/apps.css")
.module-apps.m-b-1
  .panel-header 网站应用
  if data.enableFund
    a.module-app(href="/fund")
      img(src=`/statics/apps/fund.png`)
      span= data.fundName || "基金"
  if data.user
    a.module-app(href="/exam")
      img(src=`/statics/apps/exam.png`)
      span 考试系统
  if data.showActivityEnter
    a.module-app(href="/activity")
      img(src=`/statics/apps/activity.png`)
      span 活动
  if data.siteToolEnabled
    a.module-app(href="/tools" target="_blank")
      img(src=`/statics/apps/tools.png`)
      span 计算工具
